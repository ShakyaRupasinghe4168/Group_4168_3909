<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Model\Images\download.png" />
    <None Remove="Model\Images\edu10.jpg" />
    <None Remove="Model\Images\edu15.jpg" />
    <None Remove="Model\Images\images.png" />
    <None Remove="Model\Images\key-icon.png" />
